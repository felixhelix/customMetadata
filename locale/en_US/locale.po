msgid "plugins.generic.customMetadata.displayName"
msgstr "CustomMetadata"

msgid "plugins.generic.customMetadata.tabLabel"
msgstr "Additional Metadata"

msgid "plugins.generic.customMetadata.noneCreated"
msgstr "No additional metadata created"

msgid "plugins.generic.customMetadata.settings"
msgstr "Settings"

msgid "plugins.generic.customMetadata.metadataFieldName"
msgstr "name"

msgid "plugins.generic.customMetadata.metadataFieldType"
msgstr "type"

msgid "plugins.generic.customMetadata.metadataFieldRequired"
msgstr "required"

msgid "plugins.generic.customMetadata.metadataFieldSection"
msgstr "sections"

msgid "plugins.generic.customMetadata.description"
msgstr "The CustomMetadata plugin helps adding custom metadata fields to submissions."

msgid "plugins.generic.customMetadata.update.success"
msgstr "Metadata updated successfully"

msgid "plugins.generic.customMetadata.conflictOfInterests.label"
msgstr "Competing Interests"

msgid "plugins.generic.customMetadata.conflictOfInterests.description"
msgstr "Declare possible competing interests. In the absence of any conflicts, state 'The authors declare no competing interests'."

msgid "plugins.generic.customMetadata.conflictOfInterests.noneDisclosed"
msgstr "No competing interests were disclosed."

msgid "plugins.generic.customMetadata.sourcesOfFunding.label"
msgstr "Grant Information"

msgid "plugins.generic.customMetadata.sourcesOfFunding.description"
msgstr "List any sources of funding that have supported the work. If the research did not receive any specific funding, state 'This research received no external funding'."

msgid "plugins.generic.customMetadata.sourcesOfFunding.noneDisclosed"
msgstr "The author(s) declared that no grants were involved in supporting this work."

msgid "plugins.generic.customMetadata.preRegistrationInformation.label"
msgstr "Preregistration Reference"

msgid "plugins.generic.customMetadata.preRegistrationInformation.description"
msgstr "Insert a reference to a pre-registration if applicable."

msgid "plugins.generic.customMetadata.dataAvailabilityStatement.label"
msgstr "Data Availability Statement"

msgid "plugins.generic.customMetadata.dataAvailabilityStatement.description"
msgstr "Describe where and how data underlying your research can be accessed. Any restrictions on data availability must also be disclosed. If data sharing is not applicable, state 'No underlying data are available for this article, since no datasets were generated or analysed during this study'."

msgid "plugins.generic.customMetadata.language.label"
msgstr "Primary Language"

msgid "plugins.generic.customMetadata.language.description"
msgstr "What is the primary language of your submission?"